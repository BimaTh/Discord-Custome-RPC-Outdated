

rpc.on('ready', () => {
    rpc.setActivity({
        details: "Listening to Spotify",
        startTimestamp: new Date(),
        largeImageKey: "spotify",
        largeIconText: "what are you doing here?",
        smallIconKey : "",
        smallIconText: ""
    });  
});
rpc.login({
    clientId: "768475049694724097"
})
console.log("\n RPC \'spotify\' is Online");
